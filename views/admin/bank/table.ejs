<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Card Holder Name</th>
      <th>Bank Name</th>
      <th>Card Number</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < bankData.length; i++) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= bankData[i].cardHolderName %></td>
      <td><%= bankData[i].bankName %></td>
      <td><%= bankData[i].cardNumber %></td>
      <td>
        <form
          action="/bank/delete-bank/<%= bankData[i]._id %>?_method=DELETE"
          method="POST"
        >
          <a
            href="/bank/edit-bank/<%= bankData[i]._id %>"
            class="btn btn-sm btn-warning"
            ><i class="fas fa-pencil-alt"></i> Edit</a
          >
          <button type="submit" class="btn btn-sm btn-danger">
            <i class="fas fa-trash"></i> Delete
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
